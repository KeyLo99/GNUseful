{
    "workbench.colorTheme": "Outrun Electric",
    "workbench.activityBar.visible": false,
    "workbench.statusBar.visible": true,
    "workbench.colorCustomizations": {
        "[Outrun Electric]": {
            "editorCursor.foreground": "#ffffff",
            "gitDecoration.modifiedResourceForeground": "#E6DB74",
            "terminal.ansiGreen": "#27df4e",
            "terminal.ansiRed": "#833d9c"
        },
        "[Dark++]": {
            "statusBar.background" : "#1e1e1e",
            "statusBar.noFolderBackground" : "#212121",
            "statusBar.debuggingBackground": "#263238",
        }
    },
    "editor.tokenColorCustomizations": {
        "[Outrun Electric]": {
            "comments": {
                "fontStyle": ""
            },
            "types": {
                "fontStyle": ""
            },
            "variables": {
                "foreground": "#4b76c2"
            }
        }
    },
    "editor.fontFamily": "'Borg Sans Mono', Consolas, 'Courier New', monospace",
    "editor.fontSize": 14,
    "editor.fontLigatures": true,
    "window.menuBarVisibility": "toggle",
    "window.titleBarStyle": "native",
    "files.eol": "\n",
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/node_modules": true,
        "node_modules": true,
        "target": true
    },
    "files.saveConflictResolution": "overwriteFileOnDisk",
    "rust.mode": "legacy",
    "rust.rustup": {
        "toolchain": "stable-x86_64-unknown-linux-gnu"
    },
    "git.autofetch": true,
    "tabnine.experimentalAutoImports": true,
    "discord.silent": true,
    "discord.workspaceElapsedTime": true,
    "discord.detailsEditing": "âœŽ {dirname}/{filename}:L{currentline}",
    "discord.lowerDetailsEditing": "âš™ {workspace}",
    "discord.lowerDetailsDebugging": "âš’: {workspace}",
    "discord.lowerDetailsNotFound": "none",
    "discord.detailsIdle": "chillin'",
    "discord.largeImageIdle": "chillin'",
    "discord.lowerDetailsIdle": "ðŸ˜´",
    "gitlens.modes": {
        "zen": {
            "name": "Zen",
            "statusBarItemName": "Zen",
            "description": "for a zen-like experience, disables many visual features",
            "codeLens": false,
            "currentLine": false,
            "hovers": false,
            "statusBar": false,
        },
        "review": {
            "name": "Review",
            "statusBarItemName": "Reviewing",
            "description": "for reviewing code, enables many visual features",
            "codeLens": true,
            "currentLine": true,
            "hovers": true
        }
    },
    "gitlens.mode.active": "zen",
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
      {
        "before": ["j", "j"],
        "after": ["<Esc>"]
      }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
      {
        "before": ["<leader>", "d"],
        "after": ["d", "d"]
      },
      {
        "before": ["<C-n>"],
        "commands": [":nohl"]
      }
    ],
    "vim.leader": "<space>",
    "vim.handleKeys": {
      "<C-a>": false,
      "<C-f>": false,
      "<C-h>": false,
      "<C-j>": false,
      "<C-w>": false,
    }
}